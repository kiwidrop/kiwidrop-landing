{
  "name": "kiwidrop-landing",
  "version": "0.1.0",
  "private": true,
  "source": "src/index.html",
  "scripts": {
    "start": "parcel --open",
    "build": "npm run clean-dist && parcel build --no-cache --no-source-maps",
    "build:test": "npm run clean-dist && cross-env NODE_ENV=development parcel build --no-cache",
    "debug": "concurrently \"npm:debug-*\"",
    "debug-build": "npm run clean-dist && nodemon --watch src --ext '*' --exec 'cross-env NODE_ENV=development parcel build'",
    "debug-server": "http-server ./dist -o -a localhost --proxy http://localhost:8080?",
    "test": "echo test",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "clean-dist": "rimraf ./dist"
  },
  "alias": {
    "@/*": "./src/$1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fontsource/jost": "^4.5.12",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
   
    "classnames": "^2.3.1",
   
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "framer-motion": "^6.2.6",
    "husky": "^7.0.0",
   
    "lint-staged": "^12.3.3",
   
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
   
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
  
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "react-use": "^17.3.2",
    "sass": "^1.49.7",
    "typescript": "4.5.5"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.5.0",
    "@parcel/transformer-less": "^2.6.0",
    "@parcel/transformer-sass": "^2.5.0",
    "@parcel/transformer-webmanifest": "^2.5.0",
    "buffer": "^6.0.3",
    "concurrently": "^7.2.1",
    "cross-env": "^7.0.3",
    "eslint-plugin-import": "^2.26.0",
    "http-server": "^14.1.0",
    "nodemon": "^2.0.16",
    "os-browserify": "^0.3.0",
    "parcel": "^2.5.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.14",
    "postcss-modules": "^4.3.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "util": "^0.12.4"
  }
}
